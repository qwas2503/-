        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "import xlsxwriter\n",
        "\n",
        "workbook = xlsxwriter.Workbook('kovo_record.xlsx')\n",
        "worksheet = workbook.add_worksheet()\n",
        "\n",
        "row = 0\n",
        "\n",
        "for page in range(1, 101):\n",
        "    \n",
        "    print(\"~~~~~~~~~~~~~~~~~~~~~~~~~\"+str(page))\n",
        "    url = 'https://www.kovo.co.kr/game/v-league/11141_game-summary.asp?season=017&g_part=201&r_round=1&g_num='+str(page)+'&'\n",
        "    response = requests.get(url)#고정\n",
        "    contents = BeautifulSoup(response.content,'html.parser')#고정\n",
        "    team1 = contents.select('p.th')[0].text\n",
        "    team2 = contents.select('p.th')[1].text\n",
        "    datas = contents.select('div.con.compare2.clearfix')\n",
        "\n",
        "    worksheet.write(0,0, \"팀명\")\n",
        "    worksheet.write(0,1,\"공격성공률\")\n",
        "    worksheet.write(0,2,\"리시브효율\")\n",
        "    worksheet.write(row+1,0,team1)\n",
        "    worksheet.write(row+2,0,team2)\n",
        "  \n",
        "    col = 1\n",
        "\n",
        "    for a in datas:\n",
        "        data1 = a.select('span.data')[0].text\n",
        "        data2 = a.select('span.data')[1].text\n",
        "        data3 = a.select('span.data')[2].text\n",
        "        data4 = a.select('span.data')[3].text\n",
        "\n",
        "\n",
        "        print(data1,data2, data3, data4)\n",
        "\n",
        "        worksheet.write(row+1,col,data1)\n",
        "        worksheet.write(row+2,col,data2)\n",
        "        worksheet.write(row+1,col+1,data3)\n",
        "        worksheet.write(row+2,col+1,data4)\n",
        "        \n",
        "        col += 1\n",
        "    row += 2\n",
        "    \n",
        "\n",
        "\n",
        "    \n",
        "    \n",
        "workbook.close()"
      ],
